{
  "version": "1.0",
  "canonicalRepository": "https://github.com/TrigintaFaces/Excalibur",
  "capabilityOwnership": [
    {
      "capability": "Message contracts and dispatch pipeline",
      "owner": "Dispatch",
      "dispatchPackages": [
        "Excalibur.Dispatch",
        "Excalibur.Dispatch.Abstractions"
      ],
      "excaliburPackages": [],
      "rationale": "Core message contract and dispatch execution must remain framework-agnostic."
    },
    {
      "capability": "Middleware and cross-cutting policies",
      "owner": "Dispatch",
      "dispatchPackages": [
        "Excalibur.Dispatch",
        "Excalibur.Dispatch.Patterns",
        "Excalibur.Dispatch.Resilience.Polly"
      ],
      "excaliburPackages": [],
      "rationale": "Pipeline middleware is reusable without CQRS/domain concerns."
    },
    {
      "capability": "Transport abstraction and adapters",
      "owner": "Dispatch",
      "dispatchPackages": [
        "Excalibur.Dispatch.Transport.Abstractions",
        "Excalibur.Dispatch.Transport.AwsSqs",
        "Excalibur.Dispatch.Transport.AzureServiceBus",
        "Excalibur.Dispatch.Transport.GooglePubSub",
        "Excalibur.Dispatch.Transport.Kafka",
        "Excalibur.Dispatch.Transport.RabbitMQ"
      ],
      "excaliburPackages": [],
      "rationale": "Transport adapters are independent of domain persistence and CQRS."
    },
    {
      "capability": "Serialization diagnostics hooks",
      "owner": "Dispatch",
      "dispatchPackages": [
        "Excalibur.Dispatch.Serialization.MemoryPack",
        "Excalibur.Dispatch.Serialization.MessagePack",
        "Excalibur.Dispatch.Serialization.Protobuf",
        "Excalibur.Dispatch.Observability"
      ],
      "excaliburPackages": [],
      "rationale": "Serialization and observability primitives are messaging concerns."
    },
    {
      "capability": "Minimal hosting bridge",
      "owner": "Dispatch",
      "dispatchPackages": [
        "Excalibur.Dispatch.Hosting.AspNetCore",
        "Excalibur.Dispatch.Hosting.Serverless.Abstractions"
      ],
      "excaliburPackages": [],
      "rationale": "Thin hosting bridges expose dispatcher capabilities only."
    },
    {
      "capability": "CQRS and domain orchestration",
      "owner": "Excalibur",
      "dispatchPackages": [],
      "excaliburPackages": [
        "Excalibur.Application",
        "Excalibur.Domain"
      ],
      "rationale": "CQRS composition and domain model behavior are Excalibur concerns."
    },
    {
      "capability": "Event sourcing, outbox, saga orchestration",
      "owner": "Excalibur",
      "dispatchPackages": [],
      "excaliburPackages": [
        "Excalibur.EventSourcing",
        "Excalibur.Outbox",
        "Excalibur.Saga"
      ],
      "rationale": "State persistence and orchestration policies are opinionated platform behavior."
    },
    {
      "capability": "Enterprise hosting templates",
      "owner": "Excalibur",
      "dispatchPackages": [],
      "excaliburPackages": [
        "Excalibur.Hosting",
        "Excalibur.Hosting.Web",
        "Excalibur.Hosting.AzureFunctions",
        "Excalibur.Hosting.AwsLambda",
        "Excalibur.Hosting.GoogleCloudFunctions"
      ],
      "rationale": "Production hosting defaults and templates belong in wrapper framework."
    },
    {
      "capability": "Compliance providers and key management",
      "owner": "Excalibur",
      "dispatchPackages": [
        "Excalibur.Dispatch.Compliance.Abstractions"
      ],
      "excaliburPackages": [
        "Excalibur.Compliance.SqlServer",
        "Excalibur.Compliance.Postgres"
      ],
      "rationale": "Dispatch exposes hooks while Excalibur owns concrete compliance providers."
    }
  ],
  "packageNamingPolicy": {
    "postgresCanonicalName": "Postgres",
    "legacyCompatibilityPackage": "n/a",
    "preferredPackage": "Excalibur.Data.Postgres",
    "migrationGuidance": "Use Excalibur.Data.Postgres for new integrations.",
