<Project>
  <!-- This file can be used to exclude problematic test projects during reorganization -->
  <PropertyGroup>
    <!-- Exclude old test projects with compilation issues from build -->
    <ExcludeFromBuild Condition="'$(MSBuildProjectName)' == 'Tests.Shared.Extra' or '$(MSBuildProjectName)' == 'Excalibur.Tests.Shared'">true</ExcludeFromBuild>
  </PropertyGroup>
</Project>
