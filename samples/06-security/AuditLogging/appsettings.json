{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "AuditLoggingSample": "Debug",
      "Excalibur.Dispatch.Security": "Debug"
    }
  },
  "AuditLogging": {
    "EnablePayloadLogging": true,
    "RedactSensitiveFields": true,
    "SensitiveFields": [
      "Password",
      "Secret",
      "Token",
      "CreditCard",
      "SocialSecurityNumber",
      "SSN"
    ]
  }
}
