<?xml version="1.0" encoding="utf-8"?>
<!-- Sprint 431 S431.3: AWS Secrets Manager Security Sample -->
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>AwsSecretsManagerSample</RootNamespace>
    <AssemblyName>AwsSecretsManagerSample</AssemblyName>
  </PropertyGroup>
  <!-- Dispatch Security References -->
  <ItemGroup>
    <ProjectReference Include="$(DispatchSourceRoot)Excalibur.Dispatch\Excalibur.Dispatch.csproj" />
    <ProjectReference Include="$(DispatchSourceRoot)Excalibur.Dispatch.Abstractions\Excalibur.Dispatch.Abstractions.csproj" />
    <ProjectReference Include="$(DispatchSourceRoot)Excalibur.Dispatch.Security\Excalibur.Dispatch.Security.csproj" />
    <ProjectReference Include="$(DispatchSourceRoot)Excalibur.Dispatch.Security.Aws\Excalibur.Dispatch.Security.Aws.csproj" />
  </ItemGroup>
  <!-- AWS SDK References -->
  <ItemGroup>
    <PackageReference Include="AWSSDK.SecretsManager" />
    <PackageReference Include="Microsoft.Extensions.Hosting" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" />
  </ItemGroup>
</Project>