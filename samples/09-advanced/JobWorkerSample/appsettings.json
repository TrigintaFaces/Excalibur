{
  "Logging": {
	"LogLevel": {
	  "Default": "Information",
	  "Microsoft.Hosting.Lifetime": "Information",
	  "Quartz": "Information",
	  "JobWorkerSample": "Debug"
	}
  },
  "ConnectionStrings": {
	"DefaultConnection": "",
	"Redis": ""
  },
  "Serilog": {
	"MinimumLevel": {
	  "Default": "Information",
	  "Override": {
		"Microsoft": "Warning",
		"System": "Warning",
		"Quartz": "Information"
	  }
	},
	"WriteTo": [
	  {
		"Name": "Console",
		"Args": {
		  "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
		}
	  }
	]
  }
}
