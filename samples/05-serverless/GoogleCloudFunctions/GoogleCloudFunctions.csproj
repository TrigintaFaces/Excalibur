<?xml version="1.0" encoding="utf-8"?>
<!-- Sprint 430 S430.3: Google Cloud Functions Serverless Sample -->
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <!-- OutputType=Exe is auto-generated by Google.Cloud.Functions.Hosting -->
    <OutputType>Exe</OutputType>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>GoogleCloudFunctionsSample</RootNamespace>
    <AssemblyName>GoogleCloudFunctionsSample</AssemblyName>
    <!-- Let the Functions Framework generate the entry point -->
  </PropertyGroup>

  <!-- Dispatch Hosting References -->
  <ItemGroup>
    <ProjectReference Include="$(DispatchSourceRoot)Excalibur.Dispatch\Excalibur.Dispatch.csproj" />
    <ProjectReference Include="$(DispatchSourceRoot)Excalibur.Dispatch.Abstractions\Excalibur.Dispatch.Abstractions.csproj" />
    <ProjectReference Include="$(DispatchSourceRoot)Excalibur.Dispatch.Hosting.GoogleCloudFunctions\Excalibur.Dispatch.Hosting.GoogleCloudFunctions.csproj" />
    <ProjectReference Include="$(ExcaliburSourceRoot)Excalibur.Application\Excalibur.Application.csproj" />
    <ProjectReference Include="$(ExcaliburSourceRoot)Excalibur.Hosting.GoogleCloudFunctions\Excalibur.Hosting.GoogleCloudFunctions.csproj" />
  </ItemGroup>

  <!-- Google Cloud Functions SDK References -->
  <ItemGroup>
    <PackageReference Include="Google.Cloud.Functions.Framework" />
    <PackageReference Include="Google.Cloud.Functions.Hosting" />
    <PackageReference Include="Google.Events.Protobuf" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" />
  </ItemGroup>
</Project>
