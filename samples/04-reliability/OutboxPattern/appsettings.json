{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "OutboxPattern": "Debug",
      "Excalibur.Outbox": "Debug"
    }
  },
  "Outbox": {
    "BatchSize": 50,
    "PollingIntervalSeconds": 2,
    "MaxRetryCount": 3,
    "RetryDelaySeconds": 10,
    "EnableAutomaticCleanup": true,
    "MessageRetentionHours": 1
  }
}
